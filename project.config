[project]
	description = Packaging content for django_openstack_auth (distgit)
[access "refs/*"]
  read = group django_openstack_auth-distgit-core
  owner = group django_openstack_auth-distgit-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group django_openstack_auth-distgit-core
	label-Code-Review = -2..+2 group django_openstack_auth-distgit-ptl
	label-Verified = -2..+2 group django_openstack_auth-distgit-ptl
	label-Workflow = -1..+1 group django_openstack_auth-distgit-core
	label-Workflow = -1..+1 group django_openstack_auth-distgit-ptl
	label-Workflow = -1..+0 group Registered Users
	submit = group django_openstack_auth-distgit-ptl
	read = group django_openstack_auth-distgit-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group django_openstack_auth-distgit-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
